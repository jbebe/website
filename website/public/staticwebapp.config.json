{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
	{
      "route": "/articles",
      "rewrite": "/index.html"
    },
	{
      "route": "/unknown",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
		"/fonts/*", 
		"/js/*",
		"/css/*"
	]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css",
    ".js": "text/javascript",
    ".ttf": "font/ttf"
  }
}